<w:tc><w:p><w:r><w:t>string</w:t></w:r></w:p></w:tc>
<w:tc><w:p><w:r><w:t>Yes</w:t></w:r></w:p></w:tc>
<w:tc><w:p><w:r><w:t>application/json</w:t></w:r></w:p></w:tc>
</w:tr>
<w:tr>
